<Window x:Class="otoface.GroupInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OtoFace" Height="200" Width="159">
    <StackPanel Margin="10">
        <TextBlock Text="表情グループ:" Width="111"/>
        <TextBox x:Name="txtExpression" Width="112"/>
        <TextBlock Text="キー:" Width="111"/>
        <TextBox x:Name="txtKey" Margin="0,0,0,0" Width="112"/>
        <TextBlock Text="フェードフレーム数:" Width="111"/>
        <TextBox x:Name="txtFadeFrame" Margin="0,0,0,12" Width="112"/>
        <Button Content="OK" Click="OkButton_Click" Width="111"/>
    </StackPanel>
</Window>
